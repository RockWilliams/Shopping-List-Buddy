<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
    <link rel="stylesheet" href="/styles/main.css" type="text/css">
    <link rel="stylesheet" href="/styles/home.css" type="text/css">
    <title>Document</title>
</head>
<body>
    <header>
        <h1>List Buddy</h1>
            <nav>
                <a class="models" href="/homepage">Home</a>
                <a class ="models" href="/store">Your Stores</a>
                <a class="models" href="/store/lists">Your Lists</a>
                <form action="/logout?_method=DELETE" method="POST">
                    <input type="submit" value="Logout">
                </form>
            </nav>
    </header>
    <main>
        <p>1</p>
    </main>
    <div class="flexbox">
        <div class="content">
            <h1>Stores</h1>
            <a href="/store/new">Add a Store</a>
                <% user.stores.forEach(stores => { %>
                    <p>
                        <a href="/store/<%= stores._id %>"> <%= stores.name %> </a>
                    </p>
                <% }) %>
            <form action="/logout?_method=DELETE" method="POST">
                <input type="submit" value="Logout">
            </form>
        </div>
    </div>
</body>
</html>